module base_typecheck

test type [[
[[Type]]
]] 0 errors run get-type on #1 to Type()

test fn sig 1 [[
[[_: Type -> Type]]
]] 0 errors run get-type on #1 to Type()

test fn sig 2 [[
[[_: Type -> _: Type -> Type]]
]] 0 errors run get-type on #1 to Type()

test id fn 1 [[
[[/x:Type. x]]
]] 0 errors run get-type on #1 to FnType("x", Type(), Type())

test id fn 2 [[
[[/x : (_: Type -> Type). x Type]]
]] 0 errors run get-type on #1 to FnType("x", FnType("_", Type(), Type()), Type())